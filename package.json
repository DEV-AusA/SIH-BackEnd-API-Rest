  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --verbose",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "migration:run": "npm run typeorm migration:run -- -d ./config/typeorm.config.ts",
    "migration:generate": "npm run typeorm -- -d ./config/typeorm.config.ts migration:generate ",
    "migration:create": "npm run typeorm migration:create ",
    "migration:revert": "npm run typeorm -- -d ./config/typeorm.config.ts migration:revert",
    "migration:show": "npm run typeorm -- -d ./config/typeorm.config.ts migration:show"
  },
